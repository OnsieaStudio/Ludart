	<organization>
		<name>OnsieaStudio</name>
		<url>https://github.com/OnsieaStudio/</url>
	</organization>

	<developers>
		<developer>
			<name>Seynax</name>
			<id>seynax</id>
			<email>seynax@onsiea.com</email>
			<timezone>+2</timezone>
			<organization>OnsieaStudio</organization>
			<organizationUrl>https://github.com/OnsieaStudio/</organizationUrl>
